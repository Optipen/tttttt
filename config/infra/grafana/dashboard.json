      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 6
      },
      "id": 4,
      "options": {
        "legend": {
          "calcs": ["mean", "max"],
          "displayMode": "table",
          "placement": "bottom",
          "showLegend": true
        },
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        }
      },
      "pluginVersion": "10.0.0",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "histogram_quantile(0.5, sum by (le, method) (rate(wallet_rpc_latency_seconds_bucket{instance=\"host.docker.internal:8000\"}[5m])))",
          "legendFormat": "RPC p50 ({{method}})",
          "refId": "A"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "histogram_quantile(0.95, sum by (le, method) (rate(wallet_rpc_latency_seconds_bucket{instance=\"host.docker.internal:8000\"}[5m])))",
          "legendFormat": "RPC p95 ({{method}})",
          "refId": "B"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "histogram_quantile(0.5, sum by (le) (rate(wallet_tx_scan_seconds_bucket{instance=\"host.docker.internal:8000\"}[5m])))",
          "legendFormat": "Scan p50",
          "refId": "C"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "histogram_quantile(0.95, sum by (le) (rate(wallet_tx_scan_seconds_bucket{instance=\"host.docker.internal:8000\"}[5m])))",
          "legendFormat": "Scan p95",
          "refId": "D"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "sum by (method) (rate(wallet_rpc_latency_seconds_sum{instance=\"host.docker.internal:8000\"}[5m])) / sum by (method) (rate(wallet_rpc_latency_seconds_count{instance=\"host.docker.internal:8000\"}[5m]))",
          "legendFormat": "RPC Avg ({{method}})",
          "refId": "E",
          "hide": false
        }
      ],
      "title": "RPC Latency p50/p95",
      "type": "timeseries"
